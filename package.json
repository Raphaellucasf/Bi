    "productName": "Meritus",
    "directories": {
      "output": "release",
      "buildResources": "build"
    },
    "files": [
      "build/**/*",
      "electron.js",
      "package.json"
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "public/meritus-icon.png",
      "artifactName": "Meritus-Setup-${version}.${ext}"
